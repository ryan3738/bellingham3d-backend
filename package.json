{
  "name": "bellingham-3d-frontend",
  "author": "Ryan Fraser",
  "version": "0.5.0",
  "private": true,
  "description": "Bellingham 3D e-commerce website backend! Tech used includes: keystone-6, GraphQL, react, mongodb",
  "keywords": [
    "backend",
    "keystone",
    "ecommerce"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "keystone dev",
    "start": "keystone start",
    "build": "keystone build",
    "deploy": "keystone build && keystone prisma migrate deploy",
    "predeploy": "keystone postinstall --fix",
    "postinstall": "keystone postinstall",
    "seed-data": "keystone --seed-data",
    "test": "jest --runInBand --testTimeout=60000",
    "prisma": "keystone prisma generate"
  },
  "prettier": {
    "proseWrap": "preserve",
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 100,
    "arrowParens": "avoid",
    "overrides": [
      {
        "files": "docs/**",
        "options": {
          "embeddedLanguageFormatting": "off"
        }
      }
    ]
  },
  "dependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.15.3",
    "@keystone-6/auth": "^1.0.0",
    "@keystone-6/cloudinary": "^1.0.0",
    "@keystone-6/core": "^1.0.0",
    "dotenv": "^10.0.0",
    "next": "^12.0.7",
    "nodemailer": "^6.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "stripe": "^8.176.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.12",
    "@types/nodemailer": "^6.4.4",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.4.1",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.4.3",
    "prettier": "^2.5.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.5.2"
  },
  "engines": {
    "node": "^14.15 || ^16.13"
  },
  "repository": "https://github.com/ryan3738/bellingham3d-backend"
}
